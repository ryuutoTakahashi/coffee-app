							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 201) {",
									"    var jsonData = pm.response.json();",
									"    pm.environment.set(\"userId\", jsonData.id);",
									"    pm.environment.set(\"username\", jsonData.username);",
									"}"
